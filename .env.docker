# Docker-specific environment overrides
# This file is used by docker-compose to override default .env values

# Redis connection for Docker environment
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_INDICES=0-5
REDIS_KEY_PREFIX=
REDIS_TIMEOUT=5

# Server configuration for Docker
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
SERVER_MODE=test

# Server timeouts
SERVER_TIMEOUT_READ=5
SERVER_TIMEOUT_WRITE=10
SERVER_TIMEOUT_IDLE=120
SERVER_TIMEOUT_READ_HEADER=2

# TLS disabled in development
TLS_ENABLED=false

# Application settings
HASH_KEYS=true